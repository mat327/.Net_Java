@model IEnumerable<Smart_Scale.Models.Pomiar>
@using System.Data

@{
    ViewBag.Title = "Wykres pomiarów";
}
<h3>@ViewBag.Message</h3>

<p>
    @{
        var myChart = new Chart(width: 1280, height: 720, theme: ChartTheme.Green)
.AddTitle(ViewBag.Message)
.AddSeries("Default",
    xValue: Model, xField: "Datadodania",
 yValues: Model, yFields: "Bmi")
.Write();

    }
</p>
